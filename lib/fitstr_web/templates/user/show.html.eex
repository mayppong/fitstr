<h2>Show User</h2>

<ul>

  <li>
    <strong>Handler:</strong>
    <%= @user.handler %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

  <li>
    <strong>Hashed password:</strong>
    <%= @user.hashed_password %>
  </li>

</ul>

<span><%= link "Edit", to: user_path(@conn, :edit, @user) %></span>
<span><%= link "Back", to: user_path(@conn, :index) %></span>
